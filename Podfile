     pod 'Firebase/Database'
     pod 'GoogleSignIn'
     
     # Pods for Facebook
     pod 'Bolts'
     pod 'FBSDKCoreKit'
     pod 'FBSDKLoginKit'
     pod 'MBProgressHUD'
     
     # Pods for Intercom
     pod 'Intercom'
end
